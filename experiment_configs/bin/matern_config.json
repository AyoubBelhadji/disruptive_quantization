{
    "algorithm_name": "MultipleMeanShift",
    "params": {
        "solution": {
            "K": 3,
            "freeze_init": 1
        },
        "initial_distribution": {
            "distribution_name": "gaussian_distribution",
            "params": {
                "d": 2,
                "mean": [20, 20],
                "covariance": [[1, 0], [0, 1]]
            }
        },
        "experience": {
            "R": 5
        },
        "optimization": {
            "T": 500,
            "dilation": 0.1,
            "noise_schedule_function": {
                "noise_schedule_function_name": "gaussian_sqrt_noise",
                "params": {
                    "d": 2,
                    "beta_ns": 0.0
                }
            }
        },
        "domain": {
            "class": "hypercube",
            "params": {
                "d": 2,
                "a": -25,
                "b": 25
            }
        },
        "kernel": {
            "kernel_name": "matern_kernel",
            "params": {
                "nu" :2,
                "bandwidth_schedule_function": {
                    "bandwidth_schedule_function_name": "exponential_decay_kernel_bandwidth",
                    "params": {
                        "bandwidth_decay_rate": -0.05,
                        "bandwidth_start_value": 4.5,
                        "bandwidth_end_value": 4.5
                    }
                }
            }
        },
        "dataset": {
            "dataset_name": "gmm_data",
            "N": 1000
        }
    }
}
