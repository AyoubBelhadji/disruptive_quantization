{
    "algorithm_name": "WassersteinFisherRao",
    "params": {
        "solution": {
            "K": 3,
            "freeze_init": 0
        },
        "initial_distribution": {
            "distribution_name": "gaussian_distribution",
            "params": {
                "d": 2,
                "mean": [20, 20],
                "covariance": [[1, 0], [0, 1]]
            }
        },
        "experience": {
            "R": 5
        },
        "optimization": {
            "T": 200
        },
        "kernel": {
            "kernel_name": "gaussian_kernel",
            "params": {
                "bandwidth_schedule_function": {
                    "bandwidth_schedule_function_name": "exponential_decay_kernel_bandwidth",
                    "params": {
                        "bandwidth_decay_rate": -0.05,
                        "bandwidth_start_value": 4.5,
                        "bandwidth_end_value": 4.5
                    }
                }
            }
        },
        "time_parameterization": {
            "time_discretization_name": "linear_time_parameterization",
            "params": {
                "end_time": 1e3
            }
        },
        "dataset": {
            "dataset_name": "gmm_data",
            "N": 1000
        }
    }
}
