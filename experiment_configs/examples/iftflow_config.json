{
    "algorithm_name": "InteractionForceTransportFlow",
    "params": {
        "hyperparams": {
            "centroid_step_size": 0.1,
            "weight_step_size": 1e-3
        },
        "solution": {
            "K": 3,
            "freeze_init": 0
        },
        "initial_distribution": {
            "distribution_name": "data_distribution",
            "params": {
                "d": 2
            }
        },
        "experience": {
