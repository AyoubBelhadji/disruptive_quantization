{
    "rng_seed": 42,
    "algorithm_name": "MultipleMeanShift",
    "params": {
        "hyperparams": {
            "use_adjugate": true
        },
        "solution": {
            "K": 3,
            "freeze_init": 0
        },
        "initial_distribution": {
            "distribution_name": "data_distribution",
            "params": {
                "d": 2
            }
        },
        "experience": {
            "R": 1
        },
        "optimization": {
            "T": 1000,
            "dilation": 0.1,
            "noise_schedule_function": {
                "noise_schedule_function_name": "gaussian_sqrt_noise",
                "params": {
                    "d": 2,
                    "beta_ns": 0.0
                }
            }
        },
        "domain": {
            "class": "hypercube",
            "params": {
                "d": 2,
                "a": -25,
                "b": 25
            }
        },
        "kernel": {
            "kernel_name": "matern_kernel",
            "params": {
                "nu": 2.5,
                "bandwidth": 5.0
            }
        },
        "dataset": {
            "dataset_name": "gmm_data",
            "N": 1000
        }
    }
}
