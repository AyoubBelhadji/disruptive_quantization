{
    "algorithm_name": "MultipleMeanShift",
    "params": {
        "solution": {
            "K": 3,
            "freeze_init": 1
        },
        "initial_distribution": {
            "distribution_name": "gaussian_distribution",
            "params": {
                "d": 2,
                "mean": [50, 50],
                "covariance": [[1, 0], [0, 1]]
            }
        },
        "experience": {
            "R": 2
        },
        "optimization": {
            "T": 100,
            "noise_schedule_function": {
                "noise_schedule_function_name": "gaussian_sqrt_noise",
                "params": {
                    "d": 2,
                    "beta_ns": 2
                }
            }
        },
        "domain": {
            "class": "hypercube",
            "params": {
                "d": 2,
                "a": -25,
                "b": 25
            }
        },
        "kernel": {
            "kernel_name": "gaussian_kernel",
            "params": {
                "bandwidth_schedule_function": {
                    "bandwidth_schedule_function_name": "exponential_decay_kernel_bandwidth",
                    "params": {
                        "decay_rate": -0.05,
                        "start_value": 6,
                        "end_value": 0.1
                    }
                }
            }
        },
        "dataset": {
            "dataset_name": "gmm_data",
            "N": 1000
        }
    }
}
